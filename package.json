{
  "name": "django-inertiajs",
  "version": "1.0.0",
  "description": "Django Inertia Demo",
  "main": "index.js",
  "private": true,
  "scripts": {
    "runserver": "cross-env PYTHONUNBUFFERED=1 python manage.py runserver 0.0.0.0:8000",
    "dev": "concurrently npm:start npm:runserver",
    "test_django": "python manage.py test",
    "tailwind": "npx tailwindcss -o static/css/taildwind.css",
    "start": "npx webpack --watch --progress"
  },
  "posthtml": {
    "recognizeSelfClosing": true
  },
  "watchreload": {
    "files": [
      "**/*.py",
      "**/templates/**/*.html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilfredvasquez/django-ineartia.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wilfredvasquez/django-ineartia/issues"
  },
  "homepage": "https://github.com/wilfredvasquez/django-ineartia#readme",
  "dependencies": {
    "@headlessui/vue": "^1.3.0",
    "@heroicons/vue": "^1.0.1",
    "@inertiajs/inertia": "^0.9.2",
    "@inertiajs/inertia-vue3": "^0.4.7",
    "@inertiajs/progress": "^0.2.5",
    "@tailwindcss/forms": "^0.3.3",
    "clean-webpack-plugin": "^2.0.1",
    "mini-css-extract-plugin": "^1.3.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@vue/compiler-sfc": "^3.1.2",
    "@vue/component-compiler-utils": "^3.2.2",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "glob-all": "^3.2.1",
    "postcss": "^7.0.36",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.0",
    "purgecss-webpack-plugin": "^4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.2",
    "vue-loader": "^16.2.0",
    "webpack": "^4.40.0",
    "webpack-bundle-tracker": "^1.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.2"
  }
}
