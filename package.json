{
  "name": "django-inertiajs",
  "version": "1.0.0",
  "description": "Django Inertia Demo",
  "main": "index.js",
  "private": true,
  "scripts": {
    "runserver": "cross-env PYTHONUNBUFFERED=1 python manage.py runserver 0.0.0.0:8000",
    "dev": "concurrently npm:start npm:runserver",
    "test_django": "python manage.py test",
    "tailwind": "npx tailwindcss -o static/css/taildwind.css",
    "start-web": "npx webpack --watch --progress --mode development",
    "build-web": "npx webpack"
  },
  "posthtml": {
    "recognizeSelfClosing": true
  },
  "watchreload": {
    "files": [
      "**/*.py",
      "**/templates/**/*.html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilfredvasquez/django-ineartia.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wilfredvasquez/django-ineartia/issues"
  },
  "homepage": "https://github.com/wilfredvasquez/django-ineartia#readme",
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "@heroicons/vue": "^1.0.5",
    "@inertiajs/inertia": "^0.10.1",
    "@inertiajs/inertia-react": "^0.7.1",
    "@inertiajs/inertia-vue3": "^0.5.2",
    "@inertiajs/progress": "^0.2.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@vuelidate/core": "^2.0.0-alpha.32",
    "@vuelidate/validators": "^2.0.0-alpha.25",
    "print-js": "^1.6.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.4",
    "@tailwindcss/forms": "^0.3.4",
    "@vue/compiler-sfc": "^3.2.24",
    "@vue/component-compiler-utils": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.4.0",
    "eslint-plugin-vue": "^8.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "tailwindcss": "^2.2.19",
    "terser-webpack-plugin": "^5.2.5",
    "vue-loader": "^16.8.3",
    "webpack": "^5.65.0",
    "webpack-bundle-tracker": "^1.4.0",
    "webpack-cli": "^4.9.1"
  }
}
